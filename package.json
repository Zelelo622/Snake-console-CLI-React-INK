{
	"name": "@zelelo/snake-cli-game",
	"version": "2.5.0",
	"private": false,
	"license": "MIT",
	"bin": {
		"snake-cli-game": "./dist/cli.js"
	},
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Zelelo622/Snake-console-CLI-React-INK.git"
	},
	"homepage": "https://github.com/Zelelo622/Snake-console-CLI-React-INK",
	"bugs": {
		"url": "https://github.com/Zelelo622/Snake-console-CLI-React-INK/issues"
	},
	"scripts": {
		"build": "babel --out-dir=dist source",
		"prepublishOnly": "npm run build",
		"dev": "babel --out-dir=dist --watch source",
		"test": "prettier --check . && xo && ava",
		"release": "semantic-release",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format": "prettier --write ."
	},
	"files": [
		"dist",
		"LICENSE",
		"readme.md"
	],
	"dependencies": {
		"ink": "^4.1.0",
		"ink-big-text": "^2.0.0",
		"ink-gradient": "^3.0.0",
		"meow": "^11.0.0",
		"react": "^18.2.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.21.0",
		"@babel/preset-react": "^7.18.6",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/commit-analyzer": "^13.0.1",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^12.0.2",
		"@semantic-release/npm": "^13.1.3",
		"@semantic-release/release-notes-generator": "^14.1.0",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"chalk": "^5.2.0",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^10.1.8",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-import": "^2.32.0",
		"eslint-plugin-react": "^7.37.5",
		"eslint-plugin-react-hooks": "^4.6.2",
		"globals": "^17.0.0",
		"import-jsx": "^5.0.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"semantic-release": "^25.0.2",
		"xo": "^0.53.1"
	},
	"ava": {
		"environmentVariables": {
			"NODE_NO_WARNINGS": "1"
		},
		"nodeArguments": [
			"--loader=import-jsx"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"babel": {
		"presets": [
			"@babel/preset-react"
		]
	}
}
